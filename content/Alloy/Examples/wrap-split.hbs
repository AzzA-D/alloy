<header class="w w--head">
	<div class="g">
	    <div class="c__lg--1">
	    	<h1 class="page__title">{{title}}</h1>
	    </div>
	</div>
</header>

<svg xmlns="http://www.w3.org/2000/svg" version="1.1" width="100%" height="100" viewBox="0 0 100 100" preserveAspectRatio="none" style="display:block;background-color:#F5F5F5;">
	<path d="M0 0 C 50 100 80 100 100 0 Z" style="fill:#ABB4E2;"></path>
</svg>

<main class="w w--main">

	<div class="w w--primary">
		<div class="g">
		    <div class="c__lg--5-7">
		    	PAGE
		    </div>
		    <div class="c__lg--2-7">
		    	{{> panel panel }}
